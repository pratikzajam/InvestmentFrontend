{
    "version": 2,
    "builds": [
      {
        "src": "src/**/*",
        "use": "@vercel/static-build"
      }
    ],
    "redirects": [
      {
        "source": "/old-page",
        "destination": "/new-page",
        "permanent": true
      },
      {
        "source": "/about",
        "destination": "/about-us",
        "permanent": true
      },
      {
        "source": "/blog/:slug",
        "destination": "/posts/:slug",
        "permanent": true
      }
    ]
  }
  